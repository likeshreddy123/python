def is_leap(year):
    if year % 400 == 0:
        return True
    if year % 100 == 0:
        return False
    return year % 4 == 0


def is_leap(year):
    leap = False
    
    
    if (year%400==0):
        leap=True
    elif (year%100==0):
        leap=False
    elif (year%4==0):
        leap=True
    return leap
year = int(input())
